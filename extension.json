{
	"name": "AADeckView",
	"author": [
		"saluk"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:BoilerPlate",
	"descriptionmsg": "aadeckview-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\AADeckView\\": "includes/"
	},
	"config": {
		"BoilerPlateEnableFoo": {
			"description": "Enable the Foo feature.",
			"value": true
		},
		"BoilerPlateVandalizeEachPage": {
			"description": "Write 'AADeckView was here' on each page",
			"value": false
		}
	},
	"HookHandlers": {
		"AADeckViewHooks": {
			"class": "MediaWiki\\Extension\\AADeckView\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "AADeckViewHooks"
	},
	"MessagesDirs": {
		"AADeckView": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "AADeckView/resources"
	},
	"ResourceModules": {
		"ext.aaDeckView": {
			"packageFiles": [
				"ext.aaDeckView/main.js"
			],
			"dependencies": [],
			"messages": [],
			"styles": []
		}
	},
	"manifest_version": 2
}
